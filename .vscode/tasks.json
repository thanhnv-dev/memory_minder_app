{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Setup project all sub module",
      "type": "shell",
      "command": "chmod 755 ./scripts/setup.sh && ./scripts/setup.sh",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "Generate assets constant file",
      "type": "shell",
      "command": "chmod 755 ./scripts/generate_assets_constant_file.sh && ./scripts/generate_assets_constant_file.sh",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "build_runner",
      "type": "shell",
      "command": "chmod 755 ./scripts/build_runner.sh && ./scripts/build_runner.sh",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },

    {
      "label": "flutter - ios pod",
      "type": "shell",
      "command": "cd ios && pod install && cd ..",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "flutter - ios pod update",
      "type": "shell",
      "command": "cd ios && pod install --repo-update && cd ..",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "flutter - precache ios",
      "type": "shell",
      "command": "fvm flutter precache --ios",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "flutter - pub get",
      "type": "shell",
      "command": "fvm flutter pub get",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "flutter - clean",
      "type": "shell",
      "command": "fvm flutter clean",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "flutter - pub upgrade",
      "type": "shell",
      "command": "fvm flutter pub upgrade --major-versions",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "auto_route - generated watch",
      "type": "shell",
      "command": "fvm flutter packages pub run build_runner watch",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "auto_route - generated one time",
      "type": "shell",
      "command": "fvm flutter packages pub run build_runner build",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "i18n - generate codegen_loader.g.dart",
      "type": "shell",
      "command": "chmod 755 scripts/i18n_generate_code_loader.sh && scripts/i18n_generate_code_loader.sh",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "i18n - generate file locale_keys.dart",
      "type": "shell",
      "command": "chmod 755 scripts/i18n_generate_locale_keys.sh && scripts/i18n_generate_locale_keys.sh",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    }
  ]
}
